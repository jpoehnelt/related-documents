{
  "name": "related-documents",
  "version": "1.0.0",
  "description": "Find related text documents.",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --outfile=dist/index.js --sourcemap --platform=node --external:./node_modules/* --bundle",
    "docs": "typedoc src/index.ts",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "tsc --noEmit && jest src"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "documents",
    "nlp",
    "related",
    "tfidf"
  ],
  "author": "Justin Poehnelt",
  "license": "Apache-2.0",
  "dependencies": {
    "lodash": "^4.17.21",
    "natural": "^5.1.13"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/natural": "^5.1.1",
    "esbuild": "^0.14.36",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  }
}
